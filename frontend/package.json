{
  "name": "@lexorbital/front-ring",
  "version": "0.1.0",
  "description": "LexOrbital FrontRing - Meta-Kernel frontend console",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "tests": "vitest run",
    "tests:watch": "vitest",
    "tests:ui": "vitest --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "lexorbital",
    "front-ring",
    "react",
    "typescript",
    "vite"
  ],
  "author": "@YohanGH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YohanGH/lexorbital-core"
  },
  "dependencies": {
    "i18next": "25.6.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-i18next": "13.2.1",
    "wouter": "3.8.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "4.1.17",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/ui": "2.1.8",
    "jsdom": "25.0.1",
    "tailwindcss": "4.1.17",
    "vite": "7.2.4",
    "vitest": "2.1.8"
  },
  "engines": {
    "node": ">=24.11.1"
  }
}
